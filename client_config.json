{
  "log": { "loglevel": "debug" },
  "inbounds": [
    {
      "port": 1080,
      "protocol": "socks",
      "sniffing": {
        "enabled": true,
        "destOverride": [ "http", "tls" ]
      },
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "#ARGO_URL#",
            "port": 443,
            "users": [
              {
                "id": "#UUID#",
                "alterId": 0
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "wsSettings": {
          "headers": {
            "Host": "#ARGO_URL#"
          },
          "path": "#VMESS_WSPATH#"
        }
      }
    }
  ]
}
